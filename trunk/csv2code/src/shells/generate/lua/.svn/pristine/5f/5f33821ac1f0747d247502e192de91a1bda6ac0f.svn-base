    [2048] = {["id"] = 2048, ["talk"] = "皇后娘…姐姐息怒，妹妹不曾有这种念头，近几日妹妹饮食不佳，身体虚乏，即日不曾出闺阁。", ["char"] = 0, ["name"] = "玩家", ["stance"] = 2},
    [2049] = {["id"] = 2049, ["talk"] = "既然是这样，妹妹可要好生保重玉体，皇上可是惦记妹妹惦记的很呢。那今日妹妹且委屈妹妹到各宫走走同姐妹们说说话，可别生分了。", ["char"] = 10002, ["name"] = "皇后", ["stance"] = 1},
    [2050] = {["id"] = 2050, ["talk"] = "近几日，我见花房许多花儿都开了，甚是娇嫩可爱，妹妹若是无事就同我一同前去吧。", ["char"] = 10000, ["name"] = "娴妃", ["stance"] = 1},
    [2051] = {["id"] = 2051, ["talk"] = "好啊姐姐，妹妹正想一睹皇家花房的艳丽多彩。", ["char"] = 0, ["name"] = "玩家", ["stance"] = 2},
    [2052] = {["id"] = 2052, ["talk"] = "妹妹，你看这暖阳初雪还真是像极了那皑皑白雪，不如我们也养几株吧。", ["char"] = 10000, ["name"] = "娴妃", ["stance"] = 1},
    [2053] = {["id"] = 2053, ["talk"] = "你替我传话过去谢谢娴妃姐姐，顺便陪我前去花房给花朵施施肥吧。", ["char"] = 0, ["name"] = "玩家", ["stance"] = 2},
    [2054] = {["id"] = 2054, ["talk"] = "妹妹，姐姐来打扰了。", ["char"] = 10022, ["name"] = "令嫔", ["stance"] = 1},
    [2055] = {["id"] = 2055, ["talk"] = "令嫔，魏嬿婉，原是四执库宫女，后因貌似娴妃却又丢不去婢女的软弱反得到皇上的宠幸，心机颇深，懂得迎合皇上的喜好，在水深火热的后宫中却能平步青云。", ["char"] = 10022, ["name"] = "令嫔", ["next"] = 1, ["stance"] = 1, ["narrator"] = true},
    [2056] = {["id"] = 2056, ["talk"] = "姐姐言重了，既然姐姐想去花房学习插花，那妹妹我就陪您前去花房献丑了。", ["char"] = 0, ["name"] = "玩家", ["stance"] = 2},
    [2057] = {["id"] = 2057, ["talk"] = "妹妹，姐姐听闻你时常带皇子们习武，就连咱万岁爷都时常夸你。", ["char"] = 10005, ["name"] = "惠贵妃", ["stance"] = 1},
    [2058] = {["id"] = 2058, ["talk"] = "惠贵妃，高佳晞月，原王府侧福晋，入宫后被封为贵妃，与娴妃极为不合，一心跟随皇后却依旧被皇后下毒致使终生不能生育。", ["char"] = 10005, ["name"] = "惠贵妃", ["next"] = 1, ["stance"] = 1, ["narrator"] = true},
    [2059] = {["id"] = 2059, ["talk"] = "姐姐，妹妹只是些雕虫小技何足挂口。", ["char"] = 0, ["name"] = "玩家", ["stance"] = 2},
    [2060] = {["id"] = 2060, ["talk"] = "妹妹，别这么谦虚，快去试练一下，好让姐姐也开开眼。", ["char"] = 10005, ["name"] = "惠贵妃", ["stance"] = 1},
    [2061] = {["id"] = 2061, ["talk"] = "妹妹，今日造办处要打造出一批兵器，姐姐我分身乏术，只能劳妹妹代劳姐姐前去了。", ["char"] = 10002, ["name"] = "皇后", ["stance"] = 1},
    [2062] = {["id"] = 2062, ["talk"] = "姐姐无需这样，这些是妹妹应该的。", ["char"] = 0, ["name"] = "玩家", ["stance"] = 2},
    [2063] = {["id"] = 2063, ["talk"] = "小主，您莫着急去造兵器，娴妃娘娘就料到皇后娘娘会如此，托老奴给您捎个口信，您啊，要先去造了够量的棉布再说，这兵器打造可没那么简单。", ["char"] = 10048, ["name"] = "公公", ["stance"] = 1},
    [2064] = {["id"] = 2064, ["talk"] = "用完晚膳就换上那件逍遥生吧，白衣胜雪最符合你的气质。", ["char"] = 10001, ["name"] = "皇上", ["stance"] = 1},
    [2065] = {["id"] = 2065, ["talk"] = "奴才给小主请安。", ["char"] = 10020, ["name"] = "三宝", ["stance"] = 1},
