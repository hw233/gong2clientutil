    [320] = {["id"] = 320, ["talk"] = "十分抱歉，在下准备去金陵，不认识路。这几天接连大雪到处都是一样的景致，因此误入此地。", ["char"] = {40010}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {321}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1002},
