    [301] = {["id"] = 301, ["talk"] = "小的自然知道天鹤山祭司非同小可。", ["char"] = {40020}, ["name"] = "洗砚", ["talking"] = 40020, ["next"] = {302}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
    [302] = {["id"] = 302, ["talk"] = "天鹤山是如今武林第一大派，掌门姬长生更是正道魁首，名声斐然。", ["char"] = {40020}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {303}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
    [303] = {["id"] = 303, ["talk"] = "而这大祭司正是姬长生的幼弟，虽非一母同胞，却也地位斐然，你说能让他亲自来找的人能普通吗？", ["char"] = {40020}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {304}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
