    [284] = {["id"] = 284, ["talk"] = "本座乃天鹤山祭司姬长雍，与这位颜公子的父亲是故交。他如今重伤在身，还请交于我们。在陌生人手上我们始终放心不下。", ["char"] = {40001}, ["name"] = "姬长雍", ["talking"] = 40001, ["next"] = {285}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["servantinformation"] = 10001, ["sense"] = 1002},
    [285] = {["id"] = 285, ["talk"] = "[他这身打扮，倒是不似假冒，而且也没几个人有胆量冒充天鹤山大祭司，只是颜北归对他的态度让我隐隐不安。]", ["char"] = {40001}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {286}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 10, ["special"] = {10002}},
    [286] = {["id"] = 286, ["talk"] = "原来是祭司大人，方才不知祭司大人身份，失礼之处还请祭司大人见谅。", ["char"] = {40001}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {287}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
    [287] = {["id"] = 287, ["talk"] = "无妨，不知者无罪，他没有向你提及，怨不得你。", ["char"] = {40001}, ["name"] = "姬长雍", ["talking"] = 40001, ["next"] = {288}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
    [288] = {["id"] = 288, ["talk"] = "祭司大人宽宏大量，在下感激涕零，只是……", ["char"] = {40001}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {289}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
    [289] = {["id"] = 289, ["talk"] = "说下去。", ["char"] = {40001}, ["name"] = "姬长雍", ["talking"] = 40001, ["next"] = {290}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
    [290] = {["id"] = 290, ["talk"] = "颜公子与在下早有约定，准备一同前往那金陵城中有名的来去寺小住，若是失约离去，在下委实难安。不知我们之后可否联系上颜公子。", ["char"] = {40001}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {291}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
    [291] = {["id"] = 291, ["talk"] = "想要信物，直说便是。他爹对他太过溺爱，一贯的信条在他面前都消失殆尽，本座难免要多提点训斥几句，因此生了嫌隙。", ["char"] = {40001}, ["name"] = "姬长雍", ["talking"] = 40001, ["next"] = {292}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
    [292] = {["id"] = 292, ["talk"] = "凭这块玉牌，到天鹤山来找本座便是。", ["char"] = {40001}, ["name"] = "姬长雍", ["talking"] = 40001, ["next"] = {293}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
    [293] = {["id"] = 293, ["talk"] = "[一块玉牌被掷了过来，我手忙脚乱的接住了。]", ["char"] = {40001}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {294}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002, ["special"] = {10001}},
    [294] = {["id"] = 294, ["talk"] = "自是不敢质疑祭司，如此，颜公子就交给祭司了。\n我们这便告辞了。", ["char"] = {40001}, ["name"] = "玩家", ["talking"] = -1, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
    [295] = {["id"] = 295, ["talk"] = "[我带着洗砚和承影往山下走，洗砚在我身旁欲言又止。]", ["char"] = {40020}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {296}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
    [296] = {["id"] = 296, ["talk"] = "怎么了？想说什么说就是了。", ["char"] = {40020}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {297}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
    [297] = {["id"] = 297, ["talk"] = "少爷，您刚刚不是已经答应照顾颜公子了吗，现在我们直接走了岂不是失信于人。", ["char"] = {40020}, ["name"] = "洗砚", ["talking"] = 40020, ["next"] = {298}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
    [298] = {["id"] = 298, ["talk"] = "那种形势岂是我能做主的。\n哎，若他真是颜北归，那就好了。", ["char"] = {40020}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {299}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
    [299] = {["id"] = 299, ["talk"] = "少爷你在说什么呀，那不是颜公子是谁。", ["char"] = {40020}, ["name"] = "洗砚", ["talking"] = 40020, ["next"] = {300}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1002},
