    [328] = {["id"] = 328, ["talk"] = "不过他自称颜北归，受了重伤，幸得祭司大人所救，现在应该转危为安了吧。", ["char"] = {40010}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {329}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1002},
