    [377] = {["id"] = 377, ["talk"] = "姑姑？你怎么来了？", ["char"] = {40024, 40006}, ["name"] = "雁北归", ["talking"] = 40024, ["next"] = {378}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "2", ["sense"] = 1003},
    [378] = {["id"] = 378, ["talk"] = "还不是你爹？听说你伤重，急得差点把……家里的地砖都磨穿了，还整天阴着个脸，吓得家里那些下人瑟瑟发抖。", ["char"] = {40024, 40006}, ["name"] = "重华", ["talking"] = 40006, ["next"] = {379}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "2", ["sense"] = 1003},
