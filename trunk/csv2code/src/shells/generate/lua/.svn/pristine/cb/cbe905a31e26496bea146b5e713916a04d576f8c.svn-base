    [554] = {["id"] = 554, ["talk"] = "不好了！小姐！——", ["char"] = {40021}, ["name"] = "青湖", ["talking"] = 40021, ["next"] = {555}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "3", ["sense"] = 1004},
    [555] = {["id"] = 555, ["talk"] = "冷静，青湖。\n青湖，我发现自从我回来，你与我说的最多的便是‘不好了，小姐！’。", ["char"] = {40021}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {556}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "3", ["sense"] = 1004},
    [556] = {["id"] = 556, ["talk"] = "小姐！您就知道取笑我！\n……哎不对！这次是真的不好了！\n奴婢收到消息说荀德妃娘娘派了人正在来的路上，她要寻您的麻烦啊！", ["char"] = {40021}, ["name"] = "青湖", ["talking"] = 40021, ["next"] = {557}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "3", ["sense"] = 1004},
    [557] = {["id"] = 557, ["talk"] = "……还真是不好了。", ["char"] = {40021}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {558}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "3", ["sense"] = 1004},
    [558] = {["id"] = 558, ["talk"] = "我爹在我入宫前给我分析过……", ["char"] = {40021}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {559}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "3", ["sense"] = 1004},
    [559] = {["id"] = 559, ["talk"] = "荀德妃虽说不是宫中宠妃，却是最有权势的一位。\n皇后娘娘与君上鹣鲽情深，自元后薨后，君上便不再另立皇后……\n这宫中权柄便是德妃娘娘代理执掌。", ["char"] = {40032}, ["name"] = "南鹤", ["talking"] = 40032, ["next"] = {560}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1001, ["special"] = {10014}},
    [560] = {["id"] = 560, ["talk"] = "而元后所出太子殿下更是胎中带病，降生后更是连年积病。\n当时药王孙思邈断定太子无法活过弱冠之年，就是如此也被君上一意定为太子……", ["char"] = {40032}, ["name"] = "南鹤", ["talking"] = 40032, ["next"] = {561}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1001},
    [561] = {["id"] = 561, ["talk"] = "当时朝中左右丞相都不赞同，连驳了君上三道旨意。", ["char"] = {40032}, ["name"] = "南鹤", ["talking"] = 40032, ["next"] = {562}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1001},
    [562] = {["id"] = 562, ["talk"] = "当年两位相公一位姓张，一位姓刘。", ["char"] = {40032}, ["name"] = "南鹤", ["talking"] = 40032, ["next"] = {563}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1001},
    [563] = {["id"] = 563, ["talk"] = "……我记得现在两位相公姓氏是胡和霍吧？", ["char"] = {40032}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {564}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1001},
    [564] = {["id"] = 564, ["talk"] = "……懂了懂了，君上真是铁血手腕。", ["char"] = {40032}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {565}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1001},
    [565] = {["id"] = 565, ["talk"] = "闭嘴，听我说完。\n你入宫后，一是不能得罪君上，天子一怒，浮尸万里，这不必我多说。\n再者是太子殿下，君上爱太子甚……再者是国师，再然后便是这位德妃娘娘。", ["char"] = {40032}, ["name"] = "南鹤", ["talking"] = 40032, ["next"] = {566}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1001},
    [566] = {["id"] = 566, ["talk"] = "德妃娘娘育有一子，便是公子尘来。\n如今距离太子弱冠之年仅剩半年了，到时太子……这位殿下便是最有力的继位者……", ["char"] = {40032}, ["name"] = "南鹤", ["talking"] = 40032, ["next"] = {567}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1001},
    [567] = {["id"] = 567, ["talk"] = "您的意思就是莫要跟那帮子权力中心之人有纠葛，能躲便躲？\n安心等着您把我捞出来？", ["char"] = {40032}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {568}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "3", ["sense"] = 1001},
