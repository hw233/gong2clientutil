    [532] = {["id"] = 532, ["talk"] = "国师之称，名副其实。当他毫无波澜的看向你的时候，你便觉得，那一眼似乎阅尽了你的前世今生一般。", ["name"] = "旁白", ["talking"] = 0, ["next"] = {533}, ["coordinate"] = "2", ["sense"] = 1004},
