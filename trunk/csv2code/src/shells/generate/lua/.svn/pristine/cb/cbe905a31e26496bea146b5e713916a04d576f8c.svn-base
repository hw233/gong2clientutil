    [629] = {["id"] = 629, ["talk"] = "我虽不过八品学士，却也是记录在册的女官，娘娘要杖毙我，也得有个正经理由才行。", ["char"] = {40001, 40015}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {630}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "1", ["sense"] = 1004},
