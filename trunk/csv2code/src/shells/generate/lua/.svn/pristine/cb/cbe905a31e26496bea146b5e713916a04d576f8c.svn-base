    [550] = {["id"] = 550, ["talk"] = "我翻了翻手中的团扇，满意的看着上面的牡丹纹，冲她微微一笑。\n如果不是她拿我作筏子，我也不想管这种事儿。", ["char"] = {5027}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {551}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "1", ["sense"] = 1004},
