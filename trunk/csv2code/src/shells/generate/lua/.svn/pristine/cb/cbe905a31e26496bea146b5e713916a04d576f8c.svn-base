    [380] = {["id"] = 380, ["talk"] = "来的是一名女子，穿着华美，约莫二十七八的样子。梳着妇人的发髻，脸上的笑容却很不羁，一看便知道是一个爽快泼辣的女子。", ["char"] = {40024, 40006}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {381}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "2", ["sense"] = 1003},
    [381] = {["id"] = 381, ["talk"] = "见过夫人。", ["char"] = {40024, 40006}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {382}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "2", ["sense"] = 1003},
    [382] = {["id"] = 382, ["talk"] = "哎？这位小美人儿是谁？小子，是你的红颜知己吗？你也学会金屋藏娇了？看着还不错。", ["char"] = {40024, 40006}, ["name"] = "重华", ["talking"] = 40006, ["next"] = {383}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "2", ["sense"] = 1003},
    [383] = {["id"] = 383, ["talk"] = "姑姑，这位是南府的大小姐，并不是我的红颜知己。她于我有救命之恩，还望姑姑慎言。", ["char"] = {40024, 40006}, ["name"] = "雁北归", ["talking"] = 40024, ["next"] = {384}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "2", ["sense"] = 1003},
    [384] = {["id"] = 384, ["talk"] = "南府？户部尚书南鹤家的？南家夫人所出似乎只有一男一女吗？我见过似乎并不是这位姑娘的模样。", ["char"] = {40024, 40006}, ["name"] = "重华", ["talking"] = 40006, ["next"] = {385}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "2", ["sense"] = 1003},
    [385] = {["id"] = 385, ["talk"] = "小女子长期在外求学，您未见过也是常理之中。夫人所见的，应是我的二妹南且朝。", ["char"] = {40024, 40006}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {386}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "2", ["sense"] = 1003},
    [386] = {["id"] = 386, ["talk"] = "原来如此，没想到南叶仙那老痞子还藏着这么一个如花似玉的大闺女。", ["char"] = {40024, 40006}, ["name"] = "重华", ["talking"] = 40006, ["next"] = {387, 388}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "2", ["sense"] = 1003},
    [387] = {["id"] = 387, ["talk"] = "夫人玩笑了。", ["char"] = {40024, 40006}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {388}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "2", ["sense"] = 1003},
    [388] = {["id"] = 388, ["talk"] = "正是，我爹那老痞子就是这般死命藏着掖着我这样如花似玉的闺女儿。捧着生怕摔着我，含在口中生怕我化了。故而夫人不曾见过我。", ["char"] = {40024, 40006}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {389}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "2", ["sense"] = 1003},
    [389] = {["id"] = 389, ["talk"] = "真是个有趣的小美人儿。", ["char"] = {40024, 40006}, ["name"] = "重华", ["talking"] = 40006, ["next"] = {390}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "2", ["sense"] = 1003},
    [390] = {["id"] = 390, ["talk"] = "好了，今日我有要务在身，就不与你闲聊了。燕来，跟我回去吧，你若再不肯回去，小心你爹亲自来抓你。", ["char"] = {40024, 40006}, ["name"] = "重华", ["talking"] = 40006, ["next"] = {391}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "2", ["sense"] = 1003},
    [391] = {["id"] = 391, ["talk"] = "我有伤在身，暂时不想回去，待明日我便搬去长风别院住罢，这样我爹也方便来看我。", ["char"] = {40024, 40006}, ["name"] = "雁北归", ["talking"] = 40024, ["next"] = {392}, ["stance"] = {1, 3}, ["dpos"] = {-356, -309}, ["coordinate"] = "2", ["sense"] = 1003},
