    [153] = {["id"] = 153, ["talk"] = "柳氏的眼神闪动了一下，有些不自然的挪开了目光。", ["char"] = {40018}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {154}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "1", ["sense"] = 1001},
    [154] = {["id"] = 154, ["talk"] = "大小姐为何突然询问此事？我一深宅妇人实在是不懂官场上的事情。", ["char"] = {40018}, ["name"] = "柳氏", ["talking"] = 40018, ["next"] = {155}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "1", ["sense"] = 1001},
    [155] = {["id"] = 155, ["talk"] = "既然如此，我也不便多说与太太。", ["char"] = {40018}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {156}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "1", ["sense"] = 1001},
    [156] = {["id"] = 156, ["talk"] = "太太，我府正当危难，还望太太约束下人，谨守府中，莫要失了我南府应有的气度。", ["char"] = {40018}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {157}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "1", ["sense"] = 1001},
    [157] = {["id"] = 157, ["talk"] = "大小姐这是什么意思？我……", ["char"] = {40018}, ["name"] = "柳氏", ["talking"] = 40018, ["next"] = {158}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "1", ["sense"] = 1001},
    [158] = {["id"] = 158, ["talk"] = "我摆了摆手，阻止她讲下去。", ["char"] = {40018}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {159}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "1", ["sense"] = 1001},
    [159] = {["id"] = 159, ["talk"] = "您是主母，如何做您应该明白，我不便多言。太太您是聪明人，不必我多饶口舌。", ["char"] = {40018}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {160}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "1", ["sense"] = 1001},
    [160] = {["id"] = 160, ["talk"] = "大小姐这是什么意思？！难道是我做的还不够好吗？", ["char"] = {40018}, ["name"] = "柳氏", ["talking"] = 40018, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "1", ["sense"] = 1001, ["breaktalk"] = {161}},
    [161] = {["id"] = 161, ["talk"] = "青湖，传我命令。将刚刚与春雨说话的几人还有春雨都关到柴房去，不准他们出入……我回来之前谁人都不得入内。", ["char"] = {40021}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {162}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [162] = {["id"] = 162, ["talk"] = "小姐，这……太太要是知道了……", ["char"] = {40021}, ["name"] = "青湖", ["talking"] = 40021, ["next"] = {163}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [163] = {["id"] = 163, ["talk"] = "无碍，若是我爹能顺利脱困，我就回山上……\n若是我爹不能出来……倾巢之下，焉有完卵？", ["char"] = {40021}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {164}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [164] = {["id"] = 164, ["talk"] = "是，我这就去。", ["char"] = {40021}, ["name"] = "青湖", ["talking"] = 40021, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001, ["breaktalk"] = {165}},
    [165] = {["id"] = 165, ["talk"] = "王伯，您为何在后院？", ["char"] = {40028}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {166}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1001},
    [166] = {["id"] = 166, ["talk"] = "小姐，前院有人大闹府中，坚持要见老爷。", ["char"] = {40028}, ["name"] = "王管家", ["talking"] = 40028, ["next"] = {167}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["servantinformation"] = 20004, ["sense"] = 1001},
    [167] = {["id"] = 167, ["talk"] = "什么时候的事情？", ["char"] = {40028}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {168}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1001},
    [168] = {["id"] = 168, ["talk"] = "就刚刚，那人无赖得很，现在还在前院闹腾呢，府中无人主事，便来找您拿个主意。", ["char"] = {40028}, ["name"] = "王管家", ["talking"] = 40028, ["next"] = {169}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1001},
    [169] = {["id"] = 169, ["talk"] = "找我爹？什么事儿？", ["char"] = {40028}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {170}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1001},
    [170] = {["id"] = 170, ["talk"] = "仿佛是要去救什么人，说是老爷曾欠了他长辈一份人情。", ["char"] = {40028}, ["name"] = "王管家", ["talking"] = 40028, ["next"] = {171}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1001},
    [171] = {["id"] = 171, ["talk"] = "长辈？人情？", ["char"] = {40028}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {172}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1001},
    [172] = {["id"] = 172, ["talk"] = "他有什么要求？", ["char"] = {40028}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {173}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1001},
    [173] = {["id"] = 173, ["talk"] = "无他，想要一个郎中随他去救人。", ["char"] = {40028}, ["name"] = "王管家", ["talking"] = 40028, ["next"] = {174}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1001},
    [174] = {["id"] = 174, ["talk"] = "我想了想，犹豫的小声的喊了一声。", ["char"] = {40028}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {175}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1001},
    [175] = {["id"] = 175, ["talk"] = "……承影可在？", ["char"] = {40028}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {176}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1001},
    [176] = {["id"] = 176, ["talk"] = "一道灰影从屋顶落了下来，悄无声息的落在了我的面前。", ["char"] = {40028, 40008}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {177}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "2", ["sense"] = 1001, ["special"] = {10001}},
    [177] = {["id"] = 177, ["talk"] = "王伯后退了一步，险些惊呼了一声。", ["char"] = {40028, 40008}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {178}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "2", ["sense"] = 1001},
    [178] = {["id"] = 178, ["talk"] = "我定睛去看，是一个面容清隽的灰衣男子。", ["char"] = {40028, 40008}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {179}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "2", ["sense"] = 1001},
    [179] = {["id"] = 179, ["talk"] = "属下承影见过大小姐。", ["char"] = {40028, 40008}, ["name"] = "承影", ["talking"] = 40008, ["next"] = {180}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "2", ["servantinformation"] = 10008, ["sense"] = 1001},
    [180] = {["id"] = 180, ["talk"] = "你是……承影？", ["char"] = {40028, 40008}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {181}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "2", ["sense"] = 1001},
    [181] = {["id"] = 181, ["talk"] = "男子面无表情，带着一种说不上的死寂与冷锐。", ["char"] = {40028, 40008}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {182}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "2", ["sense"] = 1001},
    [182] = {["id"] = 182, ["talk"] = "是，我就是承影。", ["char"] = {40028, 40008}, ["name"] = "承影", ["talking"] = 40008, ["next"] = {183}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "2", ["sense"] = 1001},
    [183] = {["id"] = 183, ["talk"] = "我叹了一声，五年时间，物是人非。", ["char"] = {40028, 40008}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {184}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "2", ["sense"] = 1001},
    [184] = {["id"] = 184, ["talk"] = "承影，是我南府最强之剑。\n五年前，承影还不是他。", ["char"] = {40028, 40008}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {185}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "2", ["sense"] = 1001},
    [185] = {["id"] = 185, ["talk"] = "承影，劳你随那小子走一趟，听说是要救什么人？\n若是人救回来了也就罢了，若是有什么不对，你就将那小子带回来。", ["char"] = {40028, 40008}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {186}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "2", ["sense"] = 1001},
    [186] = {["id"] = 186, ["talk"] = "是，属下遵命。", ["char"] = {40028, 40008}, ["name"] = "承影", ["talking"] = 40008, ["next"] = {187}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "2", ["sense"] = 1001},
    [187] = {["id"] = 187, ["talk"] = "王伯，带他去见那小子。", ["char"] = {40028, 40008}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {188}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "2", ["sense"] = 1001},
    [188] = {["id"] = 188, ["talk"] = "是，我这就去。", ["char"] = {40028, 40008}, ["name"] = "王管家", ["talking"] = 40028, ["next"] = {189}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "2", ["sense"] = 1001},
    [189] = {["id"] = 189, ["talk"] = "王管事没敢多问，急急忙忙的走了。我想了想，朝花园走去。", ["name"] = "旁白", ["talking"] = 0, ["next"] = {190}, ["coordinate"] = "2", ["sense"] = 1001},
    [190] = {["id"] = 190, ["talk"] = "小时候我常与我爹玩寻宝游戏，我爹最喜欢把线索藏在花园的一个角落里了……那处只有我和我爹知道，先去看看那里有没有线索。", ["name"] = "旁白", ["talking"] = 0, ["coordinate"] = "2", ["sense"] = 1001, ["breaktalk"] = {191}},
    [191] = {["id"] = 191, ["talk"] = "盒子是一封信件,拆开后，纸上只有一句话——\n慎——枫山主人。", ["name"] = "旁白", ["coordinate"] = "1", ["sense"] = 1001, ["special"] = {10009}, ["breaktalk"] = {192}},
    [192] = {["id"] = 192, ["talk"] = "我嫁入南府十数年，没想到得了大小姐如此诛心之语！", ["char"] = {40018}, ["name"] = "柳氏", ["talking"] = 40018, ["next"] = {193}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "1", ["sense"] = 1001},
    [193] = {["id"] = 193, ["talk"] = "大小姐如此不敬主母！难道不怕天打雷劈吗？", ["char"] = {40019}, ["name"] = "春雨", ["talking"] = 40019, ["next"] = {194}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "1", ["sense"] = 1001},
    [194] = {["id"] = 194, ["talk"] = "太太言重了。", ["char"] = {40018}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {195}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "1", ["sense"] = 1001},
    [195] = {["id"] = 195, ["talk"] = "如此，我便替逝去的姐姐，训诫一番大小姐！", ["char"] = {40018}, ["name"] = "柳氏", ["talking"] = 40018, ["next"] = {196}, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "1", ["sense"] = 1001},
    [196] = {["id"] = 196, ["talk"] = "呵。", ["char"] = {40018}, ["name"] = "玩家", ["talking"] = -1, ["stance"] = {1, 3}, ["dpos"] = {-356, -356}, ["coordinate"] = "1", ["sense"] = 1001, ["breaktalk"] = {197}},
    [197] = {["id"] = 197, ["talk"] = "小姐，这信……", ["char"] = {40021}, ["name"] = "青湖", ["talking"] = 40021, ["next"] = {198}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [198] = {["id"] = 198, ["talk"] = "枫山历来是皇家猎场。内里设有行宫,无事不得进出。枫山之主……恐怕也不是什么简单人物。", ["char"] = {1000}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {199}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 10},
    [199] = {["id"] = 199, ["talk"] = "可是我爹是一个绝不会无的放矢的人，我豪不怀疑这封信是我爹特意留给我看的。", ["char"] = {1000}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {200}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 10},
    [200] = {["id"] = 200, ["talk"] = "他也知道我关于推理探案这方面的智商着实不高……\n我爹万万不会在生死关头还要留点题顺道考考我——\n人不能作死，毕竟天在看。", ["char"] = {1000}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {201}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 10},
    [201] = {["id"] = 201, ["talk"] = "看来，枫山行宫是免不了要去了。", ["char"] = {1000}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {202}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 10},
    [202] = {["id"] = 202, ["talk"] = "无碍，你去找王伯，让他点些人，连夜前往枫山。", ["char"] = {1000}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {203}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [203] = {["id"] = 203, ["talk"] = "突然之间，有一个小丫鬟急匆匆跑了过来，对青湖耳语了几句。", ["char"] = {40021}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {204}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [204] = {["id"] = 204, ["talk"] = "……小姐！！！不好了！那个春雨死了！都死了！", ["char"] = {40021}, ["name"] = "青湖", ["talking"] = 40021, ["next"] = {205}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [205] = {["id"] = 205, ["talk"] = "谁死了！都死了？说清楚！", ["char"] = {40021}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {206}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [206] = {["id"] = 206, ["talk"] = "小姐你不是让我将夫人的亲信全部关起来吗……\n才抓去不到一刻钟，柴房里的人都死了！", ["char"] = {40021}, ["name"] = "青湖", ["talking"] = 40021, ["next"] = {207}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [207] = {["id"] = 207, ["talk"] = "…………\n手倒是伸得长……愚不可及！", ["char"] = {40021}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {208}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [208] = {["id"] = 208, ["talk"] = "青湖！你去吩咐王总管，就说是我说的，将夫人禁足。\n凡是夫人院里的丫头小厮，一个都不准出来，剩下的，等我回来。\n若是夫人不依，不必理她。", ["char"] = {40021}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {209}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [209] = {["id"] = 209, ["talk"] = "还有，找两个暗卫去守着。\n看着我母亲，省得我那母亲都叫人给杀了。", ["char"] = {40021}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {210}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [210] = {["id"] = 210, ["talk"] = "暗卫？哎，是承影大人吗？\n承影大人不是已经出去了吗？", ["char"] = {40021}, ["name"] = "青湖", ["talking"] = 40021, ["next"] = {211}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [211] = {["id"] = 211, ["talk"] = "……我也是气糊涂了。\n无妨，他们应该已经去了。", ["char"] = {40021}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {212}, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [212] = {["id"] = 212, ["talk"] = "是，小姐，我这就去。", ["char"] = {40021}, ["name"] = "青湖", ["talking"] = 40021, ["stance"] = {2}, ["dpos"] = {-309}, ["coordinate"] = "2", ["sense"] = 1001},
    [214] = {["id"] = 214, ["talk"] = "此处禁行，违者杀。", ["char"] = {5019}, ["name"] = "枫山守卫", ["talking"] = 5019, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1002},
    [215] = {["id"] = 215, ["talk"] = "美人儿，这枫山上可是危险得很，听哥哥的话，回家去吧。", ["char"] = {5029}, ["name"] = "游侠儿", ["talking"] = 5029, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1002},
    [216] = {["id"] = 216, ["talk"] = "这枫山上怎生这等不太平？", ["char"] = {40033}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {217}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1002},
    [217] = {["id"] = 217, ["talk"] = "想是有大事发生，我等还需小心行事。", ["char"] = {40033}, ["name"] = "侍卫", ["talking"] = 40033, ["next"] = {218}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1002},
    [218] = {["id"] = 218, ["talk"] = "大小姐，前面有一所破庙。我们是否在此休整一番？", ["char"] = {40033}, ["name"] = "侍卫", ["talking"] = 40033, ["next"] = {219}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1002},
    [219] = {["id"] = 219, ["talk"] = "也好。", ["char"] = {40033}, ["name"] = "玩家", ["talking"] = -1, ["next"] = {220}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1002},
    [220] = {["id"] = 220, ["talk"] = "我突然摆了摆手。", ["char"] = {40033}, ["name"] = "旁白", ["talking"] = 0, ["next"] = {221}, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1002},
    [221] = {["id"] = 221, ["talk"] = "噤声！", ["char"] = {40033}, ["name"] = "玩家", ["talking"] = -1, ["stance"] = {2}, ["dpos"] = {-356}, ["coordinate"] = "2", ["sense"] = 1002},
    [222] = {["id"] = 222, ["talk"] = "前面缓缓而来一队仪仗。所有人都穿着一袭白衣，内里是猩红色的内衬。他们的腰间都悬着一把青光闪闪的剑。", ["name"] = "旁白", ["talking"] = 0, ["next"] = {223}, ["coordinate"] = "2", ["sense"] = 10},
    [223] = {["id"] = 223, ["talk"] = "有人捧灯，有人持拂尘。具是低眉敛目，步伐轻缓。", ["name"] = "旁白", ["talking"] = 0, ["next"] = {224}, ["coordinate"] = "2", ["sense"] = 10},
