                   </column>
                   <column>
                        <label>领取事件参数</label>
                        <name>acEventArgs{0}</name>
                        <format>%to%,</format>
                    </column>
                </node>
                <node name="进入位面(需要初始场景)">
                    <Event/>
                    <LabelTextBox>
                        <name>传入位面</name>
                        <format>
                            <key>to</key>
                            <value>{0}</value>
                        </format>
                        <tip>示例：90  位面的SpaceNo.</tip>
                        <regex>
                            <key>2</key>
                            <value>(.*),</value>
                        </regex>
                    </LabelTextBox>
                    <LabelTextBox>
                        <name>初始场景</name>
                        <format>
                            <key>from</key>
                            <value>{0}</value>
                        </format>
                        <tip>示例：1  取消任务时回到场景的SpaceNo.</tip>
                        <regex>
                            <key>4</key>
                            <value>(.*),</value>
                        </regex>
                    </LabelTextBox>
                    <column>
                        <label>领取事件</label>
                        <name>acEvent{0}</name>
                        <format>phaseSpace</format>
                   </column>
                   <column>
                        <label>领取事件参数</label>
                        <name>acEventArgs{0}</name>
                        <format>%to%,</format>
                    </column>
                    <column>
                        <label>取消事件</label>
                        <name>cclEvent{0}</name>
                        <format>phaseSpace</format>
                   </column>
                   <column>
                        <label>取消事件参数</label>
                        <name>cclEventArgs{0}</name>
                        <format>%from%,</format>
                    </column>
                </node>
            </node>
            <node name="获得道具">
                <CheckTextBox>
                    <LabelTextBox>
                        <name>道具ID</name>
                        <format>
                            <key>id</key>
                            <value>{0}</value>
                        </format>
                        <button>10</button>
                        <tip>示例：450037  道具对应ID</tip>
                        <regex>
                            <key>acItemGain</key>
                            <value>\((\d+),\d+\),</value>
                        </regex>
                    </LabelTextBox>
                    <LabelTextBox>
                        <name>道具数量</name>
                        <format>
                            <key>count</key>
                            <value>{0}</value>
                        </format>
                        <tip>示例：1  道具数量</tip>
                        <regex>
                            <key>acItemGain</key>
                            <value>\(\d+,(\d+)\),</value>
                        </regex>
                    </LabelTextBox>
                    <name>获得道具</name>
                    <format>
                        <key>item</key>
                        <value>(%id%,%count%),</value>
                    </format>
                    <regex>
                        <key>acItemGain</key>
                        <value>\(\d+?,\d+?\),</value>
                    </regex>
                </CheckTextBox>
                <column>
                    <name>acItemGain</name>
                    <label>获得道具</label>
                    <format>%item%</format>
                </column>
                <column>
                    <name>abandonItemRm</name>
                    <label>回收道具</label>
                    <format>%item%</format>
                </column>
            </node>
            <node name="获得Buff">
