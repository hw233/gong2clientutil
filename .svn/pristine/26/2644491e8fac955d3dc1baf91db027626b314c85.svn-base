                    <Event/>
                    <LabelTextBox>
                        <name>Buff编号</name>
                        <format>
                            <key>id</key>
                            <value>{0}</value>
                        </format>
                        <button>11</button>
                        <tip>示例：39018  Buff编号</tip>
                        <regex>
                            <key>2</key>
                            <value>(.*),.*,.*,</value>
                        </regex>
                    </LabelTextBox>
                    <LabelTextBox>
                        <name>Buff等级</name>
                        <format>
                            <key>lv</key>
                            <value>{0}</value>
                        </format>
                        <tip>示例：1  Buff等级，对应状态表</tip>
                        <regex>
                            <key>2</key>
                            <value>.*,(.*),.*,</value>
                        </regex>
                    </LabelTextBox>
                    <LabelTextBox>
                        <name>Buff时限</name>
                        <format>
                            <key>time</key>
