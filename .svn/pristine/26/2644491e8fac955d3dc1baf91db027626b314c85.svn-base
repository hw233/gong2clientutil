     <AcEvent name="接任务事件"> <!-- 接任务事件 -->
            <node name="播放脚本">
                <Event/>
               <LabelTextBox>
                    <name>接任务脚本编号</name>
                    <format>
                        <key>id</key>
                        <value>"{0}.xml",</value>
                    </format>
                    <tip>示例：tidai  脚本名称</tip>
                    <regex>
                        <key>2</key>
                        <value>"(.*)\.xml",</value>
                    </regex>
                </LabelTextBox>
                <LabelTextBox>
                    <name>保护时间</name>
                    <format>
                        <key>time</key>
                        <value>{0},</value>
                    </format>
                    <tip>示例：3  数字，以秒为单位</tip>
                    <regex>
                        <key>2</key>
                        <value>".*\.xml",(\d*),?</value>
                    </regex>
                </LabelTextBox>

               <column>
                    <label>领取事件</label>
                    <name>acEvent{0}</name>
                    <format>playScenario</format>
               </column>
               <column>
                    <label>领取事件参数</label>
                    <name>acEventArgs{0}</name>
                    <format>%id%%time%</format>
                    <!-- <regular>(?<"id">"\d*.xml",)(?<"time">\d*,)</regular> -->
               </column>
            </node>
            <node name="传送">
                 <node name="传送">
                    <Event/>
                    <LabelTextBox>
                        <name>传送至坐标</name>
                        <format>
                            <key>position</key>
                            <value>{0}</value>
                        </format>
                        <tip>示例：1,23,45  pos(x,y,z)，半角逗号</tip>
                        <regex>
                            <key>2</key>
                            <value>\d*,\((.*,.*,.*)\),\(.*,.*,.*\),</value>
                        </regex>

                    </LabelTextBox>
                    <LabelTextBox>
                        <name>传送至面向</name>
                        <format>
                            <key>face</key>
                            <value>{0}</value>
                        </format>
                        <tip>示例：0,0,1.35  dir(x,y,z)，半角逗号</tip>
                        <regex>
                            <key>2</key>
                            <value>\d*,\(.*,.*,.*\),\((.*,.*,.*)\),</value>
                        </regex>

                    </LabelTextBox>
                    <LabelTextBox>
                        <name>传送至场景编号</name>
                        <format>
                            <key>id</key>
                            <value>{0}</value>
                        </format>
                        <tip>示例：1  SpaceNo，为-1时表示NPC所在场景</tip>
                        <regex>
                            <key>2</key>
                            <value>(\d*),\(.*,.*,.*\),\(.*,.*,.*\),</value>
                        </regex>
                    </LabelTextBox>
                     <column>
                        <label>领取事件</label>
                        <name>acEvent{0}</name>
                        <format>teleport</format>
                   </column>
                   <column>
                        <label>领取事件参数</label>
                        <name>acEventArgs{0}</name>
                        <format>%id%,(%position%),(%face%),</format>
                    </column>
                </node>
                <node name="传送(需要原始坐标)">
                    <Event/>
                    <LabelTextBox>
                        <name>传送至坐标</name>
                        <format>
                            <key>position</key>
                            <value>{0}</value>
                        </format>
                        <tip>示例：1,23,-45  pos(x,y,z)，半角逗号</tip>
                        <regex>
                            <key>2</key>
                            <value>\d*,\((.*,.*,.*)\),\(.*,.*,.*\),</value>
                        </regex>
                    </LabelTextBox>
                    <LabelTextBox>
                        <name>传送至面向</name>
                        <format>
                            <key>face</key>
                            <value>{0}</value>
                        </format>
                        <tip>示例：0,0,1.23  dir(x,y,z)，半角逗号</tip>
                        <regex>
                            <key>2</key>
                            <value>\d*,\(.*,.*,.*\),\((.*,.*,.*)\),</value>
                        </regex>
                    </LabelTextBox>
                    <LabelTextBox>
                        <name>传送至场景编号</name>
                        <format>
                            <key>id</key>
                            <value>{0}</value>
                        </format>
                        <tip>示例：1  SpaceNo，为-1时表示NPC所在场景</tip>
                        <regex>
                            <key>2</key>
                            <value>(\d*),\(.*,.*,.*\),\(.*,.*,.*\),</value>
                        </regex>
                    </LabelTextBox>
                    <LabelTextBox>
                        <name>初始坐标</name>
                        <format>
                            <key>pre_position</key>
                            <value>{0}</value>
                        </format>
                        <tip>示例：102,5,-91  取消任务时回到坐标，oripos(x,y,z)，半角逗号</tip>
                        <regex>
                            <key>4</key>
                            <value>\d*,\((.*,.*,.*)\),\(.*,.*,.*\),</value>
                        </regex>
                    </LabelTextBox>
                    <LabelTextBox>
                        <name>初始面向</name>
                        <format>
                            <key>pre_face</key>
                            <value>{0}</value>
                        </format>
                        <tip>示例：0,0,1.35  取消任务时回到面向，oridir(x,y,z)，半角逗号</tip>
                        <regex>
                            <key>4</key>
                            <value>\d*,\(.*,.*,.*\),\((.*,.*,.*)\),</value>
                        </regex>
                    </LabelTextBox>
                    <LabelTextBox>
                        <name>初始场景编号</name>
                        <format>
                            <key>pre_id</key>
                            <value>{0}</value>
                        </format>
                        <tip>示例：1  取消任务时回到场景，OriSpaceNo，为-1时表示NPC所在场景</tip>
                        <regex>
                            <key>4</key>
                            <value>(\d*),\(.*,.*,.*\),\(.*,.*,.*\),</value>
                        </regex>
                    </LabelTextBox>
                     <column>
                        <label>领取事件</label>
                        <name>acEvent{0}</name>
                        <format>teleport</format>
                   </column>
                   <column>
                        <label>领取事件参数</label>
                        <name>acEventArgs{0}</name>
                        <format>%id%,(%position%),(%face%),</format>
                    </column>
                    <column>
                        <label>取消事件</label>
                        <name>cclEvent{0}</name>
                        <format>teleport</format>
                   </column>
                   <column>
                        <label>取消事件参数</label>
                        <name>cclEventArgs{0}</name>
                        <format>%pre_id%,(%pre_position%),(%pre_face%),</format>
                    </column>
                </node>
            </node>
            <node name="进入位面">
                 <node name="进入位面">
                    <Event/>
                    <LabelTextBox>
                        <name>进入位面</name>
                        <format>
                            <key>to</key>
                            <value>{0}</value>
                        </format>
                        <tip>示例：90  位面的SpaceNo.</tip>
                        <regex>
                            <key>2</key>
