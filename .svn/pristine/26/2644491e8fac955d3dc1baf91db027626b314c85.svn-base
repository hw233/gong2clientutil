            <node name="NPC变怪">
                <node name="NPC变avatar(绝对坐标)">
                    <Event/>
                    <LabelTextBox>
                        <name>接任务NPC编号</name>
                        <format>
                            <key>npc_id</key>
                            <value>{0}</value>
                        </format>
                        <button>9</button>
                        <tip>示例：10144  NPCID</tip>
                        <regex>
                            <key>2</key>
                            <value>\d*,(\d*),\(\(.*\),\(.*\),\d*,\),\d*,</value>
                        </regex>
                    </LabelTextBox>
                    <LabelTextBox>
                        <name>avatar怪ID</name>
                        <format>
                            <key>monster_id</key>
