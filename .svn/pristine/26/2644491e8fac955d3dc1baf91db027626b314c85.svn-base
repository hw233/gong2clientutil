                        </regex>
                    </LabelTextBox>
                    <column>
                        <label>领取事件</label>
                        <name>acEvent{0}</name>
                        <format>becomeAvatarMonster</format>
                   </column>
                   <column>
                        <label>领取事件参数</label>
                        <name>acEventArgs{0}</name>
                        <format>%monster_id%,%npc_id%,((%position%),(%face%),%count%,),%lv%,</format>
